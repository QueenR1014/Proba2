library(tidyverse)
# Cargar datos
player_stats <- read_csv("vct_2024/players_stats/players_stats.csv")  # Cambia al nombre correcto si es diferente
# Filtrar solo el torneo relevante
acs_data <- player_stats %>%
filter(Tournament == "Valorant Champions 2024") %>%
mutate(`Average Combat Score` = as.numeric(`Average Combat Score`))  # Asegurarse que sea numérico
# Crear histograma con bins de 10
ggplot(acs_data, aes(x = `Average Combat Score`)) +
geom_histogram(binwidth = 10, fill = "#69b3a2", color = "white") +
labs(
title = "Distribución del Average Combat Score (ACS)",
x = "Average Combat Score",
y = "Número de Rendimientos"
) +
theme_minimal()
